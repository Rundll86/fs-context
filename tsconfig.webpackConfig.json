{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "config/webpack/generated",
    "moduleResolution": "node",
    "types": [
      "webpack",
      "vue",
      "node"
    ],
    "baseUrl": "."
  },
  "include": [
    "./config/webpack/**/*.ts"
  ]
}